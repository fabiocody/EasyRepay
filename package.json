{
    "name": "easyrepay-web",
    "version": "1.0.0",
    "description": "EasyRepay for the web",
    "engines": {
        "node": "14.x",
        "npm": "6.x"
    },
    "main": "src/index.ts",
    "scripts": {
        "start": "node dist/src/index.js",
        "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
        "build": "tsc",
        "test": "echo \"Error: no test specified\" && exit 1",
        "knex": "node node_modules/.bin/knex",
        "heroku-postbuild": "npm run build --prefix angular && npm run build && npm run knex migrate:latest -- --env production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fabiocody/easyrepay-web.git"
    },
    "author": "Fabio Codiglioni",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fabiocody/easyrepay-web/issues"
    },
    "homepage": "https://github.com/fabiocody/easyrepay-web#readme",
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.37",
        "@types/passport": "^1.0.6",
        "@types/passport-http": "^0.3.8",
        "@types/passport-jwt": "^3.0.5",
        "codelyzer": "^6.0.1",
        "ts-node-dev": "^1.1.6",
        "tslint": "^6.1.3",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.4",
        "morgan": "^1.10.0",
        "passport": "^0.4.1",
        "passport-http": "^0.3.0",
        "passport-jwt": "^4.0.0",
        "pg": "^8.5.1",
        "reflect-metadata": "^0.1.13",
        "sqlite3": "^5.0.2"
    }
}
